<mvvm:UserControlView x:Class="VistA.Imaging.Shell.Views.ModulesView"
           x:TypeArguments="vm:ModulesViewModel"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
           xmlns:mvvm="clr-namespace:VistA.Imaging.Prism.Views;assembly=VistA.Imaging.Prism"
           xmlns:vm="clr-namespace:VistA.Imaging.Shell.ViewModels"
           xmlns:regions="clr-namespace:Microsoft.Practices.Prism.Regions;assembly=Microsoft.Practices.Prism"
           xmlns:inf="clr-namespace:VistA.Imaging.Prism;assembly=VistA.Imaging.Prism"
           mc:Ignorable="d" 
           d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label Content="VistA Imaging" />
        <ItemsControl regions:RegionManager.RegionName="Modules" Grid.Row="1" />
    </Grid>
</mvvm:UserControlView>
