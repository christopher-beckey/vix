<?xml version="1.0" encoding="UTF-8"?>
<!--
	files: root element
	file: repeatable element for each config file to be process
		filePath: full path to a config file to operate from
		isClearText: flag to tell the program that the following "fieldToChange" elements are in clear text. If true, decrypt won't happen.
		fieldToChange: repeatable element(s) of value(s) to change
-->
<files>

    <file>
		<filePath>C:/Quoc/ModifyXML/IdConversionConfigurationTest.config</filePath>
		<isClearText>true</isClearText>
        <fieldToChange>trustStorePassword</fieldToChange>
		<fieldToChange>password</fieldToChange>
    </file>
	
    <file>
		<filePath>C:/Quoc/ModifyXML/ScpConfigurationTest.config</filePath>
		<isClearText>false</isClearText>
        <fieldToChange>accessCode</fieldToChange>
		<fieldToChange>verifyCode</fieldToChange>
    </file>
